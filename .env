# Server settings
SANDBOX_HOST=0.0.0.0
SANDBOX_PORT=8000
SANDBOX_WORKERS=4
SANDBOX_RELOAD=true
SANDBOX_LOG_LEVEL=info
SANDBOX_SSL_CERT_FILE=
SANDBOX_SSL_KEY_FILE=
SANDBOX_CORS_ORIGINS=*

# Database settings
SANDBOX_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/sandbox
SANDBOX_DB_POOL_SIZE=5
SANDBOX_DB_MAX_OVERFLOW=10
SANDBOX_DB_POOL_TIMEOUT=30
SANDBOX_DB_POOL_RECYCLE=1800

# Monitoring settings
SANDBOX_MONITOR_INTERVAL=10
SANDBOX_METRICS_ENABLED=true
SANDBOX_PROMETHEUS_PORT=9090
SANDBOX_MONITOR_CPU_THRESHOLD=90.0
SANDBOX_MONITOR_MEMORY_THRESHOLD=90.0
SANDBOX_MONITOR_DISK_THRESHOLD=90.0

# Scheduler settings
SANDBOX_CLEANUP_INTERVAL=300
SANDBOX_MAX_CONTAINER_AGE=86400
SANDBOX_INACTIVE_TIMEOUT=3600
SANDBOX_AUTO_CLEANUP_ENABLED=true

# Domain settings
SANDBOX_DOMAIN=sandbox.local
SANDBOX_DOMAIN_SCHEME=https

# Port settings
SANDBOX_PORT_RANGE_START=10000
SANDBOX_PORT_RANGE_END=20000
SANDBOX_RESERVED_PORTS=22,80,443,2375,2376,2377,2378,2379,2380,3375

# Volume settings
SANDBOX_VOLUMES_ROOT=/var/lib/sandbox/volumes

# Docker settings
SANDBOX_DOCKER_API_VERSION=1.41
SANDBOX_DOCKER_MAX_CPU=8.0
SANDBOX_DOCKER_MAX_MEMORY=16g
SANDBOX_DOCKER_DEFAULT_NETWORK=traefik-net

# Traefik settings
SANDBOX_TRAEFIK_ENTRYPOINT=websecure
SANDBOX_TRAEFIK_CERT_RESOLVER=letsencrypt
SANDBOX_TRAEFIK_HTTP_ENTRYPOINT=web
SANDBOX_TRAEFIK_HTTPS_PORT=443
SANDBOX_TRAEFIK_HTTP_PORT=80
SANDBOX_TRAEFIK_ACME_EMAIL=admin@example.com
SANDBOX_TRAEFIK_SSL_MIN_VERSION=VersionTLS12
SANDBOX_TRAEFIK_SSL_CIPHERS=TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256,TLS_ECDHE_RSA_WITH_AES_128_GCM_SHA256,TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384,TLS_ECDHE_RSA_WITH_AES_256_GCM_SHA384,TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305,TLS_ECDHE_RSA_WITH_CHACHA20_POLY1305

# Security settings
SANDBOX_MAX_CONTAINERS_PER_USER=10
SANDBOX_ENABLE_GPU=false
SANDBOX_ALLOWED_IMAGES=python:*,node:*,nginx:*
SANDBOX_BLOCKED_PORTS=22,80,443

# Resource defaults
SANDBOX_DEFAULT_CPU=1.0
SANDBOX_DEFAULT_MEMORY=512m
SANDBOX_DEFAULT_TIMEOUT=3600

# Network settings
SANDBOX_ENABLE_NETWORK=true
SANDBOX_NETWORK_ISOLATION=true 